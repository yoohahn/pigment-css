    "clsx": "^2.1.1",
    "next": "15.0.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rehype-pretty-code": "0.14.0",
    "rehype-slug": "^6.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-mdx-frontmatter": "^5.0.0",
    "shiki": "^1.22.2",
    "to-vfile": "^8.0.0",
    "vfile-matter": "^5.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-explicit-resource-management": "^7.25.9",
    "@babel/plugin-transform-unicode-property-regex": "^7.25.9",
